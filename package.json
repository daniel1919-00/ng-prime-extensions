{
  "name": "ng-prime-extensions",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-all": "ng build px-table && ng build px-uploader && ng build px-searchable-accordion",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy-docs": "ng build --base-href /ng-prime-extensions/ && npx angular-cli-ghpages docs --dir=dist/ng-prime-extensions/browser --base-href=https://github.com/daniel1919-00/ng-prime-extensions",
    "upgradeAngular": "ng cache clean && ng update --force @angular/cli @angular/core && npm install && npm update --save",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.4",
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "^20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@angular/router": "^20.1.4",
    "@ng-dominus/dm-styles": "^1.2.0",
    "ngx-highlightjs": "^14.0.1",
    "primeng": "^20.0.1",
    "@primeng/themes": "^20.0.1",
    "primeicons": "^7.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "angular-cli-ghpages": "^2.0.3",
    "eslint": "^9.32.0",
    "ng-packagr": "^20.1.0",
    "typescript": "~5.8.3"
  }
}
